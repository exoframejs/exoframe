{
  "name": "exoframe",
  "version": "4.0.1-dev",
  "description": "Exoframe is a self-hosted tool that allows simple one-command deployments using Docker",
  "main": "index.js",
  "repository": "git@github.com:exoframejs/exoframe.git",
  "author": "Tim Ermilov <yamalight@gmail.com>",
  "license": "MIT",
  "bin": {
    "exoframe": "bin/index.js"
  },
  "scripts": {
    "test": "TZ=Greenland NODE_ENV=testing jest --coverage --silent --maxWorkers=2 --ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "package": "pkg --targets node8.11.3-linux-x64,node8.11.3-win-x64,node8.11.3-macos-x64 -o exoframe index.js"
  },
  "dependencies": {
    "apache-md5": "^1.1.2",
    "boxen": "^3.2.0",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "got": "^9.6.0",
    "highland": "^2.13.4",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "multimatch": "^4.0.0",
    "open": "^6.2.0",
    "ora": "^3.4.0",
    "tar-fs": "^2.0.0",
    "update-notifier": "^2.5.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "pkg": "^4.3.8",
    "sinon": "^7.3.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
