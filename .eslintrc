{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": ["warn", "never"],
    "func-names": "off",
    "space-before-function-paren": ["error", "never"],
    "max-len": ["error", 120, 4],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
